# Contact Form API (uses custom domain api.proplastics.us)
NEXT_PUBLIC_CONTACT_API_URL=https://api.proplastics.us/contact

# Firebase Analytics API Key
# Get from Firebase Console > Project Settings > General > Your apps
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key

# Google reCAPTCHA v3
# Get from Google reCAPTCHA Admin Console: https://www.google.com/recaptcha/admin
# Create a new site with reCAPTCHA v3 and add your domains
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your_recaptcha_site_key
